{
  "env": {
    "browser": true,
    "node": true
  },
  "ecmaFeatures": {
    "jsx": true
  },
  "globals": {
    "window": true,
    "document": true
  },
  "plugins": [
  ],
  "extends": "eslint-config-airbnb",
  "rules": {
    "comma-dangle": 0,
    "no-console": 0,
    "id-length": 0,
    "react/prop-types": 0,
    "linebreak-style": 0,
    "react/prefer-stateless-function": ["warn", {"ignorePureComponents": true}]
  }
}