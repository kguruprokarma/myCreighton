{
  "name": "myCreighton",
  "version": "0.0.1",
  "description": "A digital culture that enables a truly edgeless university exhibiting Creighton's Catholic, Jesuit Mission and the One Creighton vision",
  "main": "index.js",
  "scripts": {
    "prod": "webpack -p",
    "builddev": "set NODE_ENV=dev&& webpack",
    "buildtest": "set NODE_ENV=test&& webpack",
    "build": "set NODE_ENV=production&& webpack",
    "test": "jest",
    "textlint": "eslint -c .eslintrc --ignore-path .eslintignore bluebird.min.js -f checkstyle app/**/*.js  > myCreightonEslintReport.xml || true",
    "linthtml": "eslint -c .eslintrc --ignore-path .eslintignore bluebird.min.js -f html app/**/*.js > myCreightonEslintReport.html || true",
    "lint": "npm run textlint & npm run linthtml",
    "starttest": "set NODE_ENV=test&& webpack-dev-server",
    "startdev": "set NODE_ENV=dev&& webpack-dev-server",
    "startprod": "set NODE_ENV=production&& webpack-dev-server",
    "start": "webpack-dev-server"
  },
  "jest": {
    "setupTestFrameworkScriptFile": "<rootDir>/app/__mock_i18n__/localstoarge_mock.js",
    "moduleNameMapper": {
      "\\.(css)$": "<rootDir>/node_modules/jest-css-modules"
    },
    "collectCoverage": true,
    "coverageFormats": [
      "json",
      "html"
    ]
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "axios": "^0.15.3",
    "babel-core": "^6.21.0",
    "babel-jest": "^18.0.0",
    "babel-loader": "^6.2.10",
    "babel-preset-es2015": "^6.18.0",
    "babel-preset-react": "^6.16.0",
    "babel-preset-stage-0": "^6.16.0",
    "babel-preset-stage-1": "^6.16.0",
    "babel-preset-stage-2": "^6.18.0",
    "babel-register": "^6.18.0",
    "cash-true": "0.0.2",
    "css-loader": "^0.26.1",
    "enzyme": "^2.7.0",
    "exitzero": "^1.0.1",
    "html-react-parser": "^0.3.3",
    "html-webpack-plugin": "^2.24.1",
    "i18next": "5.0.0",
    "i18next-browser-languagedetector": "1.0.1",
    "i18next-xhr-backend": "1.3.0",
    "jest": "^18.1.0",
    "json-loader": "^0.5.4",
    "lodash": "^4.17.4",
    "moment": "^2.17.1",
    "moment-timezone": "^0.5.11",
    "nodemon": "^1.11.0",
    "postcss-loader": "^1.0.0",
    "prop-types": "^15.5.8",
    "react": "^15.4.1",
    "react-avatar": "^2.3.0",
    "react-bootstrap": "^0.30.7",
    "react-bootstrap-typeahead": "^1.2.0",
    "react-custom-scrollbars": "^4.0.2",
    "react-dom": "^15.4.1",
    "react-i18next": "^2.0.1",
    "react-idle-timer": "^1.3.3",
    "react-mailto": "^0.4.0",
    "react-redux": "^4.4.5",
    "react-router": "^3.0.0",
    "react-router-bootstrap": "^0.23.1",
    "react-router-redux": "^4.0.7",
    "react-test-renderer": "^15.4.1",
    "redux": "^3.6.0",
    "redux-devtools": "^3.3.1",
    "redux-devtools-dock-monitor": "^1.1.1",
    "redux-devtools-log-monitor": "^1.0.11",
    "redux-form": "^6.5.0",
    "redux-logger": "^2.6.1",
    "redux-thunk": "^2.1.0",
    "style-loader": "^0.13.1",
    "url-loader": "^0.5.7",
    "webpack": "^1.14.0"
  },
  "devDependencies": {
    "babel-eslint": "^7.1.1",
    "babel-plugin-transform-decorators-legacy": "1.3.4",
